{
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
        "browser": false,
        "node": true
    },
    "extends": [
            "eslint:recommended"
        ],
    "rules": {
        "no-console": "off",
        "quotes": [
            "error",
            "double",
            {
                "avoidEscape": true,
                "allowTemplateLiterals": true
            }
        ],
        "strict": "off"
    }
}